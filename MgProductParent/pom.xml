<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>MgProduct</groupId>
    <artifactId>MgProductParent</artifactId>
    <version>1.0.0-SNAPSHOT</version>
    <packaging>pom</packaging>

    <properties>
        <!--处理ssl问题-->
        <java-compiler-source-ver>1.8</java-compiler-source-ver>
        <java-bcprov-jdk15on>1.52</java-bcprov-jdk15on>
        <java-bcprov-ext-jdk15on>1.52</java-bcprov-ext-jdk15on>

        <!--opt-->
        <fai-app-opt-ver>1.0.0</fai-app-opt-ver>
        <fai-comm-util-opt-ver>1.0.0</fai-comm-util-opt-ver>

        <!--arch-->
        <fai-app-arch-ver>1.0.0</fai-app-arch-ver>
        <fai-web-arch-ver>1.0.0</fai-web-arch-ver>
        <fai-cli-arch-ver>1.0.0</fai-cli-arch-ver>
        <fai-web-inf-arch-ver>1.0.0</fai-web-inf-arch-ver>
        <fai-web-kit-arch-ver>1.0.0</fai-web-kit-arch-ver>
        <fai-comm-util-arch-ver>1.0.0</fai-comm-util-arch-ver>
        <fai-comm-distributedkit-ver>1.2.0</fai-comm-distributedkit-ver>
        <fai-comm-netkit-ver>1.0.0</fai-comm-netkit-ver>
        <fai-comm-config-ver>1.0.0</fai-comm-config-ver>
        <fai-comm-mq-ver>1.0.1</fai-comm-mq-ver>
        <fai-comm-cache-ver>1.0.0</fai-comm-cache-ver>
        <fai-comm-jnetkit-ver>1.0.0</fai-comm-jnetkit-ver>
        <fai-comm-mskit-ver>1.0.0</fai-comm-mskit-ver>
        <fai-comm-zkcli-ver>1.0.0</fai-comm-zkcli-ver>
        <fai-webgateway-client-ver>1.0.0</fai-webgateway-client-ver>
        <fai-fileupload-ver>1.0.0</fai-fileupload-ver>
        <fai-ucloud-spi-arch-ver>1.0.0</fai-ucloud-spi-arch-ver>
        <fai-elasticSearch-ver>1.0.0</fai-elasticSearch-ver>
        <fai-fileupload-spi-site-ver>1.0.0</fai-fileupload-spi-site-ver>
        <fai-oauth-ver>1.0.0</fai-oauth-ver>
        <fai-spider-comm-ver>1.0.0</fai-spider-comm-ver>
    </properties>

    <!--环境-->
    <profiles>
    <!--本地环境-->
    <profile>
        <id>dev</id>
        <activation>
            <activeByDefault>true</activeByDefault>
        </activation>
        <properties>
            <repository_id>nexus-id</repository_id>
            <repository_name>snapshots repository</repository_name>
            <repository_url>http://maven.faidev.cc/repository/maven-snapshots/</repository_url>
            <!--中台-->
            <fai-comm-middleground-ver>1.0.0-SNAPSHOT</fai-comm-middleground-ver>
            <fai-middleground-infutil-ver>1.0.0-SNAPSHOT</fai-middleground-infutil-ver>
            <fai-middleground-svrutil-ver>1.0.0-SNAPSHOT</fai-middleground-svrutil-ver>

            <MgProduct-MgProductInfSvr-interfaces-ver>1.0.0-SNAPSHOT</MgProduct-MgProductInfSvr-interfaces-ver>
            <MgCommon-MgPrimaryKeySvr-interfaces-ver>1.0.0-SNAPSHOT</MgCommon-MgPrimaryKeySvr-interfaces-ver>
            <MgProduct-MgProductInfSvr-interfaces-ver>1.0.0-SNAPSHOT</MgProduct-MgProductInfSvr-interfaces-ver>
            <MgProduct-MgProductPropSvr-interfaces-ver>1.0.0-SNAPSHOT</MgProduct-MgProductPropSvr-interfaces-ver>
            <MgProduct-MgProductSearchSvr-interfaces-ver>1.0.0-SNAPSHOT</MgProduct-MgProductSearchSvr-interfaces-ver>
            <MgProduct-MgProductBasicSvr-interfaces-ver>1.0.0-SNAPSHOT</MgProduct-MgProductBasicSvr-interfaces-ver>
            <MgProduct-MgProductSpecSvr-interfaces-ver>1.0.0-SNAPSHOT</MgProduct-MgProductSpecSvr-interfaces-ver>
            <MgProduct-MgProductStoreSvr-interfaces-ver>1.0.0-SNAPSHOT</MgProduct-MgProductStoreSvr-interfaces-ver>
            <MgProduct-MgProductGroupSvr-interfaces-ver>1.0.0-SNAPSHOT</MgProduct-MgProductGroupSvr-interfaces-ver>
            <MgProduct-MgProductComm-ver>1.0.0-SNAPSHOT</MgProduct-MgProductComm-ver>
            <MgProduct-MgProductInfUtil-ver>1.0.0-SNAPSHOT</MgProduct-MgProductInfUtil-ver>
        </properties>
    </profile>

        <!--正式环境-->
        <profile>
            <id>pro</id>
            <properties>
                <repository_id>nexus-id</repository_id>
                <repository_name>release repository</repository_name>
                <repository_url>http://maven.faidev.cc/repository/maven-releases/</repository_url>
                <!--中台-->
                <fai-comm-middleground-ver>1.0.0</fai-comm-middleground-ver>
                <fai-middleground-svrutil-ver>1.0.0</fai-middleground-svrutil-ver>
                <MgCommon-MgPrimaryKeySvr-interfaces-ver>1.0.0</MgCommon-MgPrimaryKeySvr-interfaces-ver>
                <MgProduct-MgProductInfSvr-interfaces-ver>1.0.0</MgProduct-MgProductInfSvr-interfaces-ver>
                <MgProduct-MgProductPropSvr-interfaces-ver>1.0.0</MgProduct-MgProductPropSvr-interfaces-ver>
                <MgProduct-MgProductBasicSvr-interfaces-ver>1.0.0</MgProduct-MgProductBasicSvr-interfaces-ver>
                <MgProduct-MgProductSpecSvr-interfaces-ver>1.0.0</MgProduct-MgProductSpecSvr-interfaces-ver>
                <MgProduct-MgProductStoreSvr-interfaces-ver>1.0.0</MgProduct-MgProductStoreSvr-interfaces-ver>
                <MgProduct-MgProductGroupSvr-interfaces-ver>1.0.0</MgProduct-MgProductGroupSvr-interfaces-ver>
                <MgProduct-MgProductComm-ver>1.0.0</MgProduct-MgProductComm-ver>
                <fai-middleground-infutil-ver>1.0.0</fai-middleground-infutil-ver>
            </properties>
        </profile>
    </profiles>
    <distributionManagement>
        <repository>
            <id>${repository_id}</id>
            <name>${repository_name}</name>
            <url>${repository_url}</url>
        </repository>

        <snapshotRepository>
            <id>${repository_id}</id>
            <name>${repository_name}</name>
            <url>${repository_url}</url>
        </snapshotRepository>
    </distributionManagement>
</project>
